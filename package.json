{
	"name": "i-helper",
	"displayName": "i-helper",
	"description": "develop helper",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:view.common",
		"onView:view.other"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "i-helper.viewCommon",
				"title": "显示常用说明"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "iHelper",
					"title": "前端工具箱",
					"icon": "resources/images/i-helper.svg"
				}
			]
		},
		"views": {
			"iHelper": [
				{
					"id": "view.common",
					"name": "常用说明"
				},
				{
					"id": "view.other",
					"name": "其他"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.60.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"@vscode/test-electron": "^1.6.2"
	}
}